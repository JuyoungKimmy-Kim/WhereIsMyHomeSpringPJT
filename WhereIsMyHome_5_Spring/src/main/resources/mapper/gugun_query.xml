<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycom.myapp.code.dao.GugunDao"> <!--  @Mapper annotation 있는 dao -->
	
	<!-- 시도 코드로 구군 리스트 받아오기 -->
	<select id="gugunList" resultType="com.mycom.myapp.code.dto.GugunDto">
		select code, name, sido_code
		  from gugun_code
		 where sido_code= #{sidoCode}
	</select>

</mapper>